    "axios": "^1.2.6",
    "crypto-js": "^4.1.1",
    "element-plus": "2.2.28",
    "lodash-es": "^4.17.21",
    "matcher": "^5.0.0",
    "mockjs": "^1.1.0",
    "nanoid": "^4.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.25",
    "pinia-plugin-persist": "^1.0.0",
    "string-template": "^1.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
